.floor_container .floor_group .floor_title {
  border-left: 10rpx solid #fff;
  border-right: 10rpx solid #fff;
  padding: 10rpx 0;
}
.floor_container .floor_group .floor_title image {
  width: 100%;
}
.floor_container .floor_group .floor_list {
  border-left: 10rpx solid #fff;
  border-right: 10rpx solid #fff;
  overflow: hidden;
}
.floor_container .floor_group .floor_list navigator {
  float: left;
  width: 33.33%;
}
.floor_container .floor_group .floor_list navigator:nth-last-child(-n + 4) {
  height: calc((100vw - 20rpx) / 3 * 386 / 232 / 2);
  border-left: 10rpx solid #fff;
}
.floor_container .floor_group .floor_list navigator:nth-child(2) {
  border-bottom: 10rpx solid #fff;
}
.floor_container .floor_group .floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.floor_container .floor_group .floor_list navigator image {
  width: 100%;
  height: 100%;
}
